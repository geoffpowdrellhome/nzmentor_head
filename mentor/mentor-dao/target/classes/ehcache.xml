<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../lib/ehcache/ehcache.xsd">
    <diskStore path="java.io.tmpdir"/>
  
	<!--
		Mandatory Default Cache configuration. The defaultCache has an
		implicit name "default" which is a reserved cache name.
	-->
	<defaultCache
            maxElementsInMemory="10000"
            eternal="false"
		    timeToIdleSeconds="120"
            timeToLiveSeconds="120"
            overflowToDisk="true"
		    diskSpoolBufferSizeMB="30"
            maxElementsOnDisk="1"
            diskPersistent="false"
		    diskExpiryThreadIntervalSeconds="120"
            memoryStoreEvictionPolicy="LRU" />

	<!--
		Cache for JQ fares. Expiration: 12 hours since last use, or 24 hours since
		creation, or 10,000 elements
	-->
	<cache name="fares"
        maxElementsInMemory="10000" eternal="false" timeToIdleSeconds="43200" timeToLiveSeconds="86400" overflowToDisk="false"
		diskPersistent="false" />

    <!--
		Cache for cities. Expiration: 15 minutes since last use, or 1 hour since
		creation, or 500 elements
	-->
    <cache name="cities"
        maxElementsInMemory="1000" eternal="false" timeToIdleSeconds="900" timeToLiveSeconds="3600" overflowToDisk="false"
        diskPersistent="false" />
        
    <!--
        @Cacheable for terminals. Expiration: 15 minutes since last use, or 1 hour since
        creation, or 500 elements
    -->
    <cache name="terminals"
        maxElementsInMemory="5000" eternal="false" timeToIdleSeconds="900" timeToLiveSeconds="3600" overflowToDisk="false"
        diskPersistent="false" />
        
    <!--
        @Cacheable for terminal types. Expiration: 15 minutes since last use, or 1 hour since
        creation, or 500 elements
    -->
    <cache name="terminalTypes"
        maxElementsInMemory="10" eternal="false" timeToIdleSeconds="900" timeToLiveSeconds="3600" overflowToDisk="false"
        diskPersistent="false" />
    
    <!--
      Cache for countries. Expiration: 15 minutes since last use, or 1 hour since
      creation, or 500 elements
    -->
    <cache name="countries"
        maxElementsInMemory="500" eternal="false" timeToIdleSeconds="900" timeToLiveSeconds="3600" overflowToDisk="false"
        diskPersistent="false" />
        
    <!--
      Cache for regions. Expiration: 15 minutes since last use, or 1 hour since
      creation, or 500 elements
    -->
    <cache name="regions"
        maxElementsInMemory="100" eternal="false" timeToIdleSeconds="900" timeToLiveSeconds="3600" overflowToDisk="false"
        diskPersistent="false" />
    
    <!--
      Cache for flightData. Expiration: 12 hours since last use, or 24 hours since
      creation (a week), or 10000 elements
    -->
    <cache name="flightData"
        maxElementsInMemory="10000" eternal="false" timeToIdleSeconds="43200" timeToLiveSeconds="86400" overflowToDisk="false"
        diskPersistent="false" />
    
    <cache name="sites"
        maxElementsInMemory="10" eternal="true" overflowToDisk="false" diskPersistent="false" />

    <!-- @Cacheable -->
    <cache name="airlineFareBases"
        maxElementsInMemory="10000"
        eternal="true"
        overflowToDisk="false" />

    <!-- @Cacheable -->
    <cache name="timeZoneOffsets"
        maxElementsInMemory="100" eternal="false" timeToIdleSeconds="900" timeToLiveSeconds="3600" overflowToDisk="false"
        diskPersistent="false" />
    
    <!-- @Cacheable -->
